{
  "name": "nuxt-blog-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "sync-hydra": "cp ../../packages/hydra-js/hydra.js ./packages/hydra.js",
    "prebuild": "npm run sync-hydra",
