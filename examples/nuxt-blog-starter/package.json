    "nuxt": "^3.20.2",
    "nuxt-security": "^2.0.0",
    "sharp": "^0.34.5",
    "vite": "^5.4.21",
    "vite-plugin-mkcert": "^1.17.6",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
